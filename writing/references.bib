	pages = {359--366},
	file = {Snapshot:C\:\\Users\\csahi\\Zotero\\storage\\S35KYWFV\\1520-6807(200007)374367AID-PITS73.0.html:text/html;Submitted Version:C\:\\Users\\csahi\\Zotero\\storage\\X58UXH2D\\Ferrari and Scher - 2000 - Toward an understanding of academic and nonacademi.pdf:application/pdf},
