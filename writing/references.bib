	note = {Number: 10
Publisher: Nature Publishing Group},
	keywords = {Human behaviour, Psychology, Computer science, Information technology},
	pages = {1096--1106},
	file = {Full Text PDF:C\:\\Users\\csahi\\Zotero\\storage\\MQ4KEE43\\Lieder et al. - 2019 - Cognitive prostheses for goal achievement.pdf:application/pdf},
}

@article{shu_procrastination_2010,
